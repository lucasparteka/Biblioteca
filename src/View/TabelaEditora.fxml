<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../style/application.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="View.TabelaEditoraController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="tabelaEditora" prefHeight="282.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="colunaID" prefWidth="95.0" text="ID" />
                <TableColumn fx:id="colunaNome" prefWidth="338.0" text="Nome" />
                  <TableColumn fx:id="colunaNacionalidade" prefWidth="166.0" text="Nacionalidade" />
              </columns>
            </TableView>
            <Button fx:id="botaoSelecionar" layoutX="186.0" layoutY="321.0" mnemonicParsing="false" onAction="#selecionarEditora" text="Selecionar" />
            <Button fx:id="botaoCancelar" layoutX="315.0" layoutY="321.0" mnemonicParsing="false" onAction="#cancelarAcao" text="Cancelar" />
         </children>
      </Pane>
   </children>
</AnchorPane>
